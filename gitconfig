  editor = vim
  excludesfile = ~/.gitignore_global
  eol = lf
  autocrlf = no
[diff]
  tool = vimdiff
[format]
  pretty = %C(yellow)%h%C(green)%d%Creset %s %C(red)(%an, %cr)%Creset
